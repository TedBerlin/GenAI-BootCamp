    "from collections import defaultdict\n",
    "\n",
    "word = input(\"Entrez un mot : \")\n",
    "letter_indexes = defaultdict(list)\n",
    "\n",
    "for index, letter in enumerate(word):\n",
    "    letter_indexes[letter].append(index)\n",
    "\n",
    "print(dict(letter_indexes))"
